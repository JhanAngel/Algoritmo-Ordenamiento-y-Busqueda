package Packet;

import java.util.Scanner;

public class Algoritmo {

    public static void main(String[] args) {
        Scanner entrada = new Scanner(System.in);
        
        int Elementos = 7;  // Se define el tama침o del arreglo
        int arreglo[] = new int[Elementos];  // Inicializaci칩n del arreglo con el tama침o definido
        
        for (int i = 0; i < Elementos; i++) {  // Iterar sobre el tama침o del arreglo
            System.out.println("Numero valorico del arreglo " + (i + 1) + ": ");
            arreglo[i] = entrada.nextInt();
        }
        
        //Metodo burbuja
        
        
        
        
        
    }
}
